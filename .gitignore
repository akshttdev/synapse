imagebind/
imagebind-huge/
imagebind_huge/
imagebind-large/
models/imagebind/
models/

# ImageBind model weights
*.pth
*.pt
*.bin
*.safetensors

# Specific ImageBind huge model
imagebind_huge.pth
backend/imagebind/imagebind_huge.pth
models/imagebind_huge.pth


